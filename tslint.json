{
    "extends": [
        "tslint:recommended",
        "tslint-immutable",
        "tslint-clean-code",
        "tslint-sonarts"
    ],
    "rules": {
        "member-ordering": false,
        "object-literal-sort-keys": false,
        "no-namespace": false,
        "ordered-imports": false,
        "ban-types": false,
        "member-access": [true, "no-public"],
        "variable-name": [true, "allow-leading-underscore"]
    }
}
