{
    "name": "ts-class-autobind",
    "version": "0.2.6",
    "keywords": [
        "react",
        "autobind",
        "typescript"
    ],
    "description": "Method auto-bind for TypeScript",
    "main": "lib/autobind.js",
    "typings": "lib/autobind.d.ts",
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "lint": "tslint -p tsconfig.json",
        "test": "cross-env NODE_ENV=testing nyc mocha",
        "test:debug:win": "cross-env NODE_ENV=testing nyc mocha %NODE_DEBUG_OPTION%",
        "test:debug:linux": "cross-env NODE_ENV=testing nyc mocha $NODE_DEBUG_OPTION",
        "prepublish": "npm run build"
    },
    "author": "arutiunian666@gmail.com",
    "license": "ISC",
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.9.0",
        "chai": "^4.1.2",
        "cross-env": "^5.2.0",
        "husky": "^1.0.0-rc.13",
        "mocha": "^5.2.0",
        "nyc": "^12.0.2",
        "prettier": "1.14.2",
        "pretty-quick": "^1.6.0",
        "source-map-support": "^0.5.9",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-clean-code": "^0.2.7",
        "tslint-immutable": "^4.6.0",
        "tslint-sonarts": "^1.7.0",
        "typescript": "^3.0.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run test && pretty-quick --staged && npm run lint"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/DavidArutiunian/ts-class-autobind.git"
    },
    "bugs": {
        "url": "https://github.com/DavidArutiunian/ts-class-autobind/issues"
    },
    "homepage": "https://github.com/DavidArutiunian/ts-class-autobind#method-auto-bind-for-typescript"
}
